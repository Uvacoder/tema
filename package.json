{
  "name": "tema",
  "version": "1.0.0",
  "description": "Tema",
  "scripts": {
    "test": "npm run js:lint && npm run css:lint",
    "build": "webpack --config prod.config.js",
    "start": "webpack-dev-server --open",
    "deploy": "npm run build && gh-pages -d dist",
    "lint": "npm run js:lint && npm run css:lint",
    "lint:fix": "npm run js:fix && npm run css:fix",
    "js:lint": "eslint src --ext .ts",
    "js:fix": "eslint src --ext .ts --fix",
    "css:lint": "stylelint **/*.scss",
    "css:fix": "stylelint **/*.scss --fix",
    "type-coverage": "typescript-coverage-report"
  },
  "author": "yoksel",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:yoksel/tema.git"
  },
  "bugs": {
    "url": "https://github.com/yoksel/tema/issues"
  },
  "homepage": "https://github.com/yoksel/tema",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-eslint": "^10.0.3",
    "css-loader": "^3.4.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^5.0.2",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.2",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^19.0.0",
    "ts-loader": "^8.0.2",
    "type-coverage": "^2.9.0",
    "typescript": "^3.9.7",
    "typescript-coverage-report": "^0.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.1"
  }
}
